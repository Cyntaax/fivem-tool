{
    "branches": ["master"],
    "tagFormat": "${version}",
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        ["@semantic-release/github", {
            "assets": [
                {
                    "path": "./output/fivem-tool.exe",
                    "label": "Windows x64 executable"
                }
            ]
        }]
    ]
}